{
	"PHPDoc: @inheritDoc (proposed)": {
		"prefix": "@inheritDoc",
		"body": [
			"/**",
			" * @inheritDoc",
			" */"
		]
	},
	"PHPDoc: @inheritDoc": {
		"prefix": "@inheritDoc",
		"body": [
			"/**",
			" * ${1:Summary}",
			" *",
			" * {@inheritDoc}",
			" *",
			" * ${2:Extended description}",
			" */"
		]
	},
	"PHPDoc: @deprecated": {
		"prefix": "@deprecated",
		"body": "@deprecated Use {@see $0} instead"
	},
	"PHPDoc: constructor summary": {
		"prefix": "Creates",
		"body": "Creates a new ${TM_FILENAME_BASE/[^a-zA-Z0-9]+//g} object"
	},
	"PHPStan: assert": {
		"prefix": "@phpstan",
		"body": "@phpstan-assert ${1:Type, e.g. !null} $0"
	},
	"PHPStan: assert if true": {
		"prefix": "@phpstan",
		"body": "@phpstan-assert-if-true ${1:Type, e.g. !null} $0"
	},
	"PHPStan: ignore next line": {
		"prefix": "@phpstan",
		"body": "@phpstan-ignore-next-line"
	},
	"PHPStan: impure method": {
		"prefix": "@phpstan",
		"body": "@phpstan-impure"
	},
	"PHPStan: require implements": {
		"prefix": "@phpstan",
		"body": "@phpstan-require-implements $0"
	},
	"PHPStan: dump type": {
		"prefix": "dump",
		"body": "\\PHPStan\\dumpType($0);"
	}
}