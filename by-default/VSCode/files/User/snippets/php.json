{
	"PHPDoc: @inheritDoc (proposed)": {
		"prefix": "@inheritDoc",
		"body": [
			"/**",
			" * @inheritDoc",
			" */"
		]
	},
	"PHPDoc: @inheritDoc": {
		"prefix": "@inheritDoc",
		"body": [
			"/**",
			" * ${1:Summary}",
			" *",
			" * {@inheritDoc}",
			" *",
			" * ${2:Extended description}",
			" */"
		]
	},
	"PHPDoc: @api": {
		"prefix": "@api",
		"body": [
			"/**",
			" * @api",
			" */"
		]
	},
	"PHPDoc: @internal": {
		"prefix": "@internal",
		"body": [
			"/**",
			" * @internal",
			" */"
		]
	},
	"PHPDoc: @deprecated": {
		"prefix": "@deprecated",
		"body": "@deprecated Use {@see $0} instead"
	},
	"PHPDoc: constructor summary": {
		"prefix": "Creates",
		"body": "Creates a new ${TM_FILENAME_BASE/[^a-zA-Z0-9]+//g} object"
	},
	"PHPStan: assert not null": {
		"prefix": "@phpstan",
		"body": "@phpstan-assert !null $0"
	},
	"PHPStan: assert not null if true": {
		"prefix": "@phpstan",
		"body": "@phpstan-assert-if-true !null $0"
	},
	"PHPStan: ignore": {
		"prefix": "@phpstan",
		"body": "@phpstan-ignore $0"
	},
	"PHPStan: impure method": {
		"prefix": "@phpstan",
		"body": "@phpstan-impure"
	},
	"PHPStan: require implements": {
		"prefix": "@phpstan",
		"body": "@phpstan-require-implements $0"
	},
	"PHPStan: dump type": {
		"prefix": "dump",
		"body": "\\PHPStan\\dumpType($0);"
	},
	"Intelephense: @disregard P1006": {
		"prefix": "@disregard",
		"body": "/** @disregard P1006 */"
	}
}