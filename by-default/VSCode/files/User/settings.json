    "launch": {
        "configurations": [
            {
                "name": "PHP: Listen for Xdebug",
                "type": "php",
                "request": "launch",
                "port": 9003,
                "xdebugSettings": {
                    "max_children": 10000,
                    "show_hidden": 1,
                    "max_data": 10000000
                },
                "skipFiles": [
                    "!**/vendor/lkrms/**",
                    "!**/vendor/salient/**",
                    "**/vendor/**"
                ]
            },
            {
                "name": "PHP: Current file",
                "type": "php",
                "request": "launch",
                "program": "${file}",
                "cwd": "${fileDirname}",
                "port": 0,
                "runtimeExecutable": "php",
                "runtimeArgs": [
                    "-dzend_extension=xdebug.so",
                    "-dxdebug.start_with_request=yes"
                ],
                "env": {
                    "XDEBUG_MODE": "debug,develop",
                    "XDEBUG_CONFIG": "client_port=${port}"
                },
                "xdebugSettings": {
                    "max_children": 10000,
                    "show_hidden": 1,
                    "max_data": 10000000
                },
                "skipFiles": [
                    "!**/vendor/lkrms/**",
                    "!**/vendor/salient/**",
                    "**/vendor/**"
                ]
            },
            {
                "name": "PHP: Current file, with optional arguments",
                "type": "php",
                "request": "launch",
                "program": "${file}",
                "cwd": "${fileDirname}",
                "port": 0,
                "runtimeExecutable": "bash",
                "runtimeArgs": [
                    "-c",
                    "eval \"args=(${*: -1})\" && php \"${@:1:$#-1}\" \"${args[@]}\"",
                    "bash",
                    "-dzend_extension=xdebug.so",
                    "-dxdebug.start_with_request=yes"
                ],
                "env": {
                    "XDEBUG_MODE": "debug,develop",
                    "XDEBUG_CONFIG": "client_port=${port}"
                },
                "xdebugSettings": {
                    "max_children": 10000,
                    "show_hidden": 1,
                    "max_data": 10000000
                },
                "skipFiles": [
                    "!**/vendor/lkrms/**",
                    "!**/vendor/salient/**",
                    "**/vendor/**"
                ],
                "args": [
                    "${input:phpArgs}"
                ]
            }
        ],
        "inputs": [
            {
                "id": "phpArgs",
                "type": "command",
                "command": "memento.promptString",
                "args": {
                    "id": "phpArgs",
                    "description": "Command line arguments"
                }
            }
        ]
    },
