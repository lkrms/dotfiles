#!/usr/bin/env bash

. "$df_root/bin/lib/bash-helpers.sh" || exit

file=files/workspace6/General/.dbeaver/credentials-config.clean.json
file=$(find_first "$file") || exit 2
replace=${file%.clean.json}.json

backup_file "$replace"
replace_file "$replace" < <(
    {
        printf '0123456789abcdef'
        jq -c <"$file"
    } | openssl aes-128-cbc -K babb4a9f774ab853c96c2d653dfe544a -iv 00000000000000000000000000000000
)
